# Azure CLI Create Container
rgName='az204-rg'
accountName='NameOfAccount'
dbName='NameOfDatabase'
containerName='ContainerName'
partitionKey='/PartitionKey'
throughput=400

az cosmosdb sql container create \
    -a $accountName \
    -g $rgName \
    -d $dbName \
    -n $containerName \
    -p $partitionKey \
    --throughput $throughput